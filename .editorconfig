root = true

[*]
charset = utf-8
end_of_line = crlf
indent_size = 2
tab_width = 2
indent_style = tab
insert_final_newline = false
max_line_length = 120

[*.rs]
max_line_length = 100

[{*.har,*.jsb2,*.jsb3,*.json,.babelrc,.eslintrc,.stylelintrc,bowerrc,jest.config}]
indent_size = 2

[{*.yaml,*.yml}]
indent_size = 2
